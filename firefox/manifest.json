{
  "manifest_version": 2,
  "name": "WordleBot Bypass",
  "version": "0.0.2",
  "description": "Bypass Wordle Bot paywall and review your wordle stats",
  "content_scripts": [
    {
      "js": [
        "content_script.js"
      ],
      "matches": [
        "https://www.nytimes.com/*"
      ]
    }
  ],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "homepage_url": "https://add0n.com/mobile-view.html"
}