{
   "manifest_version": 3,
   "name": "WordleBot Bypass",
   "version": "0.0.2",
   "action": {
      "default_popup": "popup.html",
      "default_icon": {
         "128": "icon.png"
      }
   },
   "icons": {
      "128": "icon.png"
   },
   "permissions": [
      "storage"
   ],
   "content_scripts": [
      {
         "js": [
            "content_script.js"
         ],
         "matches": [
            "https://www.nytimes.com/*"
         ]
      }
   ]
}
